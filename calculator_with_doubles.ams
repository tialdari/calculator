.data
	prompt: .asciiz "\nWprowadz pierwsza liczbe: "
	prompt2: .asciiz "\nWprowadz druga liczbe: "

.text

	li $v0, 4
	la $a0, prompt
	syscall
	
	li $v0, 7
	syscall
	
	li $v0, 3
	add.d $f12, $f0, $f10
	syscall
	
	mov.d $f2, $f12

	li $v0, 4
	la $a0, prompt2
	syscall
	
	li $v0, 7
	syscall
	
	li $v0, 3
	add.d $f12, $f0, $f10
	syscall
	
	mov.d $f4, $f12
	

	
	li $v0, 3
	add.d $f12, $f4, $f2
	syscall