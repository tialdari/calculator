.data
	prompt: .asciiz "\nWprowadz pierwsza liczbe: "
	prompt2: .asciiz "\nWprowadz druga liczbe: "

.text

	
	li $v0, 4
	la $a0, prompt
	syscall
	
	li $v0, 7
	syscall
	

	add.d $f12, $f0, $f10

	
	mov.d $f2, $f12

	li $v0, 4
	la $a0, prompt2
	syscall
	
	li $v0, 7
	syscall
	

	add.d $f12, $f0, $f10

	
	mov.d $f4, $f12
		
	li $v0, 3
	sub.d $f12, $f2, $f4
	syscall